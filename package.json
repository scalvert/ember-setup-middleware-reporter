{
  "name": "@scalvert/ember-setup-middleware-reporter",
  "version": "0.0.1",
  "description": "Utilities to setup server middleware for Ember apps and addons",
  "keywords": [
    "ember",
    "middleware"
  ],
  "homepage": "https://github.com/scalvert/ember-setup-middleware-reporter#readme",
  "bugs": {
    "url": "https://github.com/scalvert/ember-setup-middleware-reporter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scalvert/ember-setup-middleware-reporter.git"
  },
  "license": "MIT",
  "author": "scalvert",
  "type": "module",
  "exports": {
    ".": "./lib/index.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "!lib/**/*-test.*"
  ],
  "scripts": {
    "build": "tsc",
    "docs": "node ./scripts/generate-api-docs.js",
    "lint": "eslint . --ext js,ts",
    "pretest": "npm run build",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.12",
    "body-parser": "^1.19.0",
    "errorhandler": "^1.5.1",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node-fetch": "^2.5.12",
    "@types/tmp": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "jsdoc-to-markdown": "^7.0.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0",
    "tmp": "^0.2.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "12.22.4",
    "npm": "7.20.3"
  }
}
